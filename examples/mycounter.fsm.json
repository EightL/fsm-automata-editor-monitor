{
    "name":"COUNTER",
    "inputs":["inc","dec","reset"],
    "outputs":["value"],
    "variables":[{"name":"val","type":"int","init":0}],
    "states":[
      { "id":"RUN", "initial":true,
        "onEnter":"output(\"value\", val);"
      }
    ],
    "transitions":[
      {"from":"RUN","to":"RUN","trigger":"inc","onTrigger":"val++;"},
      {"from":"RUN","to":"RUN","trigger":"dec","onTrigger":"val--;"},
      {"from":"RUN","to":"RUN","trigger":"reset","onTrigger":"val=0;"}
    ]
  }
  